RewriteEngine On
RewriteBase /nsa-uno-website/
RewriteRule ^historik(/?)$ historik.php [L]
RewriteRule ^recipe/([0-9]*)/?$ recipe.php?id=$1 [L]
RewriteRule ^login(/?)$ login.php [L]

<Files .htaccess>
Order Allow,Deny
Deny from all
</Files>

<Files "dbAccess.php">
Order Allow,Deny
Deny from all
</Files>